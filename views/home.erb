<div class="flex-container">
    <% @data.data.each do |key, items| %>
    <div class="flex-card">
            <p><%= key %></p>
            <ul>
            <% items.each do |item| %>
                <li><%= item %></li>
            <% end %>                
            </ul>
    </div>
    <% end %>
</div>
